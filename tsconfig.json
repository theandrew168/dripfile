{
	"compilerOptions": {
		"esModuleInterop": true,
		"isolatedModules": true,
        "jsx": "react",
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": true,
		"target": "esnext"
	},
	"include": [
		"frontend"
	]
}

