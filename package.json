{
	"name": "dripfile",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite build --watch",
		"build": "tsc && vite build",
		"format": "prettier --write frontend/"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.19",
		"@heroicons/react": "^2.2.0",
		"@tanstack/react-query": "^5.62.7",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router": "^6.22.0",
		"react-router-dom": "^6.28.0"
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.9",
		"@types/react": "^18.3.16",
		"@types/react-dom": "^18.3.5",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.49",
		"prettier": "^3.4.2",
		"tailwindcss": "^3.4.16",
		"typescript": "^5.7.2",
		"vite": "^5.4.11"
	},
	"postcss": {
		"plugins": {
			"tailwindcss": {},
			"autoprefixer": {}
		}
	},
	"prettier": {
		"arrowParens": "always",
		"bracketSameLine": false,
		"bracketSpacing": true,
		"jsxSingleQuote": false,
		"printWidth": 120,
		"semi": true,
		"singleQuote": false,
		"trailingComma": "all",
		"useTabs": true
	}
}
